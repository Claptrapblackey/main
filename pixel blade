-- Queue the script for after teleport with a 5 second delay
queue_on_teleport([[
    wait(5)
    loadstring(game:HttpGet('https://raw.githubusercontent.com/TexRBLX/Roblox-stuff/refs/heads/main/pixel%20blade/final.lua'))()
]])

-- Execute it immediately now
loadstring(game:HttpGet("https://raw.githubusercontent.com/TexRBLX/Roblox-stuff/refs/heads/main/pixel%20blade/final.lua"))()
